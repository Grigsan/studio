{
    "entities": {
      "CardItem": {
        "title": "Card Item",
        "description": "Represents a custom card created by a user.",
        "type": "object",
        "properties": {
          "label": {
            "type": "string",
            "description": "The text label for the card."
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        },
        "required": ["label", "createdAt"]
      }
    },
    "auth": {
      "providers": []
    },
    "firestore": {
      "/cards/{cardId}": {
        "schema": {
          "$ref": "#/backend/entities/CardItem"
        },
        "description": "Stores custom cards shared by all users."
      }
    }
}
